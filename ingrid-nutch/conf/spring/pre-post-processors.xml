<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="queryExtensionPreProcessor" class="de.ingrid.utils.processor.impl.QueryExtensionPreProcessor" />
    
    <!-- this post processor is not needed anymore since normalization is done in ibus -->
    <!-- <bean id="normalizeScoreProcessor" class="de.ingrid.utils.processor.impl.NormalizeScoreProcessor" /> -->
    
	<bean id="preProcessors"
		class="org.springframework.beans.factory.config.ListFactoryBean">
		<property name="sourceList">
			<list>
				<ref bean="queryExtensionPreProcessor" />
			</list>
		</property>
	</bean>
    <bean id="postProcessors"
		class="org.springframework.beans.factory.config.ListFactoryBean">
		<property name="sourceList">
			<list>
                <!--<ref bean="normalizeScoreProcessor" />-->
            </list>
		</property>
	</bean>
</beans>